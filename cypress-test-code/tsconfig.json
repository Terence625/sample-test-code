{
  "include": ["**/*.ts"],
  "compilerOptions": {
    "jsx": "react-jsx",
    "target": "es5",
    "types": ["cypress", "cypress-plugin-snapshots", "node"],
    "lib": ["es2015", "dom"],
    "isolatedModules": false,
    "allowJs": true,
    "noEmit": true,
    "resolveJsonModule": true,
    "moduleResolution": "node",
    "esModuleInterop": true
  }
}
